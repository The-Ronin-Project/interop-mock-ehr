<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.projectronin.interop" level="INFO"/>
    <logger name="com.projectronin.interop.mock" level="DEBUG"/>
    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
